{
  "name": "yardpassword",
  "version": "0.1.0",
  "description": "a password generator",
  "private": true,
  "main": "main/main.js",
  "scripts": {
    "clean": "rimraf dist .next out",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "electron:dev": "electron . dev",
    "electron": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "out",
      "!node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lyyyttooon/yardpassword.git"
  },
  "keywords": [
    "electron",
    "password",
    "react"
  ],
  "author": "Lyyyttooon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lyyyttooon/yardpassword/issues"
  },
  "homepage": "https://github.com/Lyyyttooon/yardpassword#readme",
  "dependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "antd": "^5.4.2",
    "autoprefixer": "10.4.14",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "electron": "^24.1.2",
    "electron-builder": "^24.3.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8"
  }
}
